{
  "name": "@feakin/cli",
  "version": "0.2.0-alpha.1",
  "description": "cli",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/phodal/@feakin/cli",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-p build:*",
    "test": "react-scripts test --watchAll=false"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {

  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "dependencies": {
    "@feakin/exporter": "^0.2.0",
    "@feakin/parser": "^0.2.0"
  }
}
